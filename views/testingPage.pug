- var Address = "Address: "
- var City = "City: "
- var Zip = "ZIP: "
- var Cost = "Price: "
- var Bedroom = "Bedromms: "

doctype html
html(lang="en")
  head
    title "Test Page"

    body
      h1 "Header"
      p "Paragraph"
        a(href="link") This is the link text to be clicked

      form(method='GET', action="/fa17g08/property")
        div.form-group
          label(for='searchInput') Search:
          input(name="searchInput", type="text", id="ourid", placeholder="Search...")
          input(type="submit", value="Button")


      div
        ul
          li Here is the Property:
          li #{Address} #{propertyList[1].Address}       
          li #{City} #{propertyList[1].City}
          li #{Zip} #{propertyList[1].ZipCode}
          li #{Cost} #{propertyList[1].Cost}
          li #{Bedroom} #{propertyList[1].Bedrooms}
          
